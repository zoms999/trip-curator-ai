{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git",
        "--repository",
        "github.com/zoms999/"
      ],
      "disabled": true,
      "alwaysAllow": []
    },
    "mcp-playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@adalovu/mcp-playwright",
        "--key",
        "b1a7aff1-d24f-4790-9b08-2275c239f5a8",
        "--profile",
        "awake-cockroach-TRaLe1"
      ],
      "disabled": true,
      "alwaysAllow": []
